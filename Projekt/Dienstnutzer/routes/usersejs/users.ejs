<html>
<head>
    <title>Users</title>
</head>
<body>
    <h1>Users</h1>
    <ul>
        <%
            for(var i = 0;i < benutzer.length; i++){
                %><li><a href="/users/<%=benutzer[i].id%>"><%= benutzer[i].name %></a></li><%
            }
        %>
    </ul>
    <% if(benutzer[0] == undefined) { %><p>Dieses System verfügt über keine User.</p><%}%>
    
    
    <form action="/users" method="POST">
  <input type="text" name="name">
  <input type="submit">
</form>
</body>
</html>